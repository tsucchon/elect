{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
  "functions": {
    "backend/api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/main.py"
    }
  ]
}
